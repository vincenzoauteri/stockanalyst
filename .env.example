# PostgreSQL
POSTGRES_DB=stockanalyst
POSTGRES_USER=stockanalyst
POSTGRES_PASSWORD=defaultpassword

# Flask
FLASK_ENV=development

# API Keys
FMP_API_KEY=
GEMINI_API_KEY=

# Scheduler
ENABLE_SCHEDULER=true

# Development container user mapping
# These should match the host user's UID and GID to avoid permission issues.
USER_UID=1000
USER_GID=1000

# Docker group ID for Docker socket access
# On Linux, you can get this value by running: echo $(getent group docker | cut -d: -f3)
DOCKER_GID=
